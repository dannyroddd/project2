<!DOCTYPE html>
<html>
    <%- include("../partials/head.ejs")%>
  <body>
      
    <%- include("../partials/header.ejs") %>
        <nav>
          <a href="/workouts/new"><button class="add">Add a New Workout</button></a>
          <a href="/user/logout"><button>Logout</button></a>
        </nav>
          <ul>
          <% for (workout of workouts) { %>
  
            <li>
            <a href="/workouts/<%= workout._id %>">
               <nav class="namebtn"> <button class="name">
                <%= workout.name %> 
            </button></nav> </a>
              <a href="/workouts/<%= workout._id %>">
              <img src="<%= workout.img%>" alt="">
            </a>
        </li>
  
          <% } %>
        
    </ul>

    <div class="stats">
       <h5 class="cal">Average Calories Burned per Basic Routine: 461 Calories</h5>
    <h4 class="day">Days Completed:</h4>
    <input type="text" id="day" value="0">
  

    <button onclick="addDay()" class="addDay"> Add Day +</button>
    </div>

   
    </body>
</html>